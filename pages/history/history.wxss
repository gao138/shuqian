/**errer.wxss**/
page{
  background: #f0f0f0;
}
.container .header {
  background: #fc5c1e;
  color: #fff;
}

.container .header {
  background: #fc5c1e;
}

.container .header header {
  justify-content: flex-start;
}

.container .header image {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  z-index: 10;
}
.container .header .back{
  height: 100%;
  display: flex;
  align-items: center;
}
.container .header .back image {
  width: 20rpx;
  margin: 0 20rpx;
}

.container .header header span {
  display: block;
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
  font-weight: normal;
}
.container .lists .wrap{
  margin-top:30rpx;
  background: #fff;
}
.container .lists .wrap .lis{
  line-height: 160rpx;
  height: 160rpx;
  display: flex;
  padding: 0 20rpx;
}
.container .lists .wrap .lis text:first-child{
  text-align: left;
  font-size: 20rpx;
  color: #676767;
  flex: 1;
}
.container .lists .wrap .lis text:last-child{
  text-align: right;
  font-size: 36rpx;
  color: #fb5c24;
  flex: 1;
}