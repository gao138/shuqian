page{
  background: #f0f0f0;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
} 
.container .header {
  background: #fc5c1e;
  color: #fff;
}

.container .header {
  background: #fc5c1e;
}

.container .header header {
  justify-content: flex-start;
}

.container .header header span {
  display: block;
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
  font-weight: normal;
}

.container .btn{
  margin: 0 auto;
  width: 602rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 24rpx;
  background: #fb710f;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
  margin-top: 80rpx;
}