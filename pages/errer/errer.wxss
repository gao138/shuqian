/**errer.wxss**/

page {
  width: 100%;
  height: 100%;
  overflow: hidden;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #f0f0f0;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.container .header {
  background: #fc5c1e;
}

.container .header header {
  justify-content: flex-start;
  position: relative;
}

.container .header image {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  z-index: 10;
}

.container .header .back, .container .header .err {
  height: 100%;
  display: flex;
  align-items: center;
}

.container .header .back image {
  width: 20rpx;
  margin: 0 20rpx;
}

.container .header header span {
  display: block;
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
  font-weight: normal;
  color: #fff;
}

.container .errList {
  display: flex;
  flex-direction: column;
  background: #fff;
  margin-top: 20rpx;
}

.container .errList a {
  height: 88rpx;
  color: #333;
  font-size: 24rpx;
  line-height: 88rpx;
  text-indent: 20rpx;
  border-bottom: 1px solid #e3e3e3;
}

.container .fycontent {
  display: flex;
  background: #fff;
  margin-top: 30rpx;
}

.container .fycontent textarea {
  flex: 1;
  font-size: 26rpx;
}

.container .btn {
  margin: 0 auto;
  width: 602rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 24rpx;
  background: #fb710f;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
  margin-top: 80rpx;
}
