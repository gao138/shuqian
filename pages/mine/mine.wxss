/**ok.wxss**/
 page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
} 
.container .header{
  background: #fc5c1e;
}
.container .header header{
    justify-content: flex-start;
    position: relative;
}
.container .header image{
  width: 40rpx;
  height: 40rpx;
}
.container .header .back{
  height: 100%;
  display: flex;
  align-items: center;
}
.container .header .back image{
  width: 20rpx;
  margin: 0 20rpx;
}

.container .content{
  background: #fff;
}
.container .content .title{
  height: 550rpx;
  background: #f4f8fb;
  font-size: 42rpx;
  color: #333;
  overflow: hidden;
}
.container .content .title image{
  display: block;
  width: 206rpx;
  height: 206rpx;
  border-radius: 50%;
  margin: 114rpx auto 0;
}
.container .content .title span{
  display: block;
  font-size: 37rpx;
  text-align: center;
  margin-top: 50rpx;
}
.container .content .list view{
  padding-left: 60rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  height: 88rpx;
  border-bottom: 1px solid #eaeaea;
}
.container .content .list view image{
  width: 44rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.container .content .list view:nth-child(2) image{
  height: 34rpx;
}
.container .content .list button view image{
  width: 34rpx;
  height: 36rpx;
  margin-left: 5rpx;
  margin-right: 25rpx;
}
.container .content .list view:nth-child(4) image{
  width: 34rpx;
  height: 42rpx;
  margin-left: 5rpx;
  margin-right: 25rpx;
}
.container .content .bg image{
  height: 370rpx;
  width: 514rpx;
  margin-top: 80rpx;
}
.container .content .list view .surplus{
  font-size: 20rpx;
  color: #bebebe;
  padding-left: 10rpx;
}

button{
  border-radius:0;
  overflow:hidden;
  background-color:transparent;
  border: 1px solid transparent;
  padding: 0;
}
button::after{ border: none; }

