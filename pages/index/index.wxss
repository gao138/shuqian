.container .content{
  background: #f4f4f4;
}
.container .content .wrap{
  padding-top: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.container .content .wrap .box{
  margin-bottom: 30rpx;
  width: 224rpx;
  height: 160rpx;
  position: relative;
}
.container .content .wrap .box image{
    display: block;
}
.container .content .wrap .box>image{
  width: 224rpx;
  height: 160rpx;
  border-radius: 24rpx;
}
.container .content .wrap .box .cont{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  display: flex;
  flex-direction: column;
  text-align: center; 
  font-size: 26rpx;
  color: #fff;
}
.container .content .wrap .box .cont span{
  height: 107rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}

