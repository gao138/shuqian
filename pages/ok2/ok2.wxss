/**ok.wxss**/
 page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
} 
.container .header{
  background: #fc5c1e;
}
.container .header header{
    justify-content: flex-start;
    position: relative;
}
.container .header image{
  width: 40rpx;
  height: 40rpx;
}
.container .header .back,
.container .header .err{
  height: 100%;
  display: flex;
  align-items: center;
}
.container .header .back image{
  width: 20rpx;
  margin: 0 20rpx;
}

.container .content{
  background: #b2b2b2;
}
.container .content .store{
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  position: relative;
  top: 100rpx;
}
.container .content .allNum{
  text-align: center;
  color: #fff;
  font-weight: bold;
  font-size: 48rpx;
  margin-top: 124rpx;
  min-height: 64rpx;
}
.container .content .boxImg{
  width: 684rpx;
  height: 684rpx;
  overflow: hidden;
  margin: 0 auto;
  border-radius: 50%;
  margin-top: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.container .content .boxImg image{
  width: 658rpx;
  height: 658rpx;
  border-radius: 50%;
}
.container .content .btns{
  margin-top: 120rpx;
  display: flex;
  justify-content: center;
}
.container .content .btns text{
  width: 236rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  text-align: center;
  border-radius: 12rpx;
  background: #2a91fe;
}
