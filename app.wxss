/**app.wxss**/
/*头部返回键图标字体*/

@font-face {
	font-family: "iconfont";
	src: url('//at.alicdn.com/t/font_640627_78eoa6elzhncdi.eot?t=1524537468838');
	/* IE9*/
	src: url('//at.alicdn.com/t/font_640627_78eoa6elzhncdi.eot?t=1524537468838#iefix') format('embedded-opentype'), /* IE6-IE8 */
	url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'), url('//at.alicdn.com/t/font_640627_78eoa6elzhncdi.ttf?t=1524537468838') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
	url('//at.alicdn.com/t/font_640627_78eoa6elzhncdi.svg?t=1524537468838#iconfont') format('svg');
	/* iOS 4.1- */
}

.iconfont {
	font-family: "iconfont" !important;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-back:before {
	content: "\e617";
  color: #fff;
}

.container .header{
  background: #fefefe;
}
.container .header header{
  display: flex;
	justify-content: center;
	align-items: center;
  position: relative;
  height: 45px;
}
.container .header header .goback {
	position: absolute;
	left: 20rpx;
	top: 0;
	display: inline-block;
	line-height: 90rpx;
	height: 90rpx;
  font-size: 36rpx;
}
.container .header header span{
  font-weight: bold;
  font-size: 36rpx;
}
.container .content{
  flex: 1;
  position: relative;
} 
